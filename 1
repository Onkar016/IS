string = input("Enter String: ")
res_and=""
res_xor=""
for char in string:
    and_val = ord(char)&127
    xor_val = ord(char)^127
    res_and += chr(and_val)
    res_xor += chr(xor_val)
    
print("Original String: ",string)
print("And Resulr: ",res_and)
print("XOR result: ",res_xor)



a = input(str("Enter a string: "))
b = 127
c = b and a
d = [chr(ord(x)^ord(y)) for x,y in zip(a,str(b))]
print(c)
print(d)
